<view  class="container">
    <view  class="page-body">
        <input  bindinput="titleInput" class="input" type="text" placeholder="请输入页面标题并点击设置即可">{onekit_2}</input>
        <onekit_button  bindtap="setNavigationBarTitle" type="primary" hover-stop-propagation="true">设置</onekit_button>
    </view>
    <view  class="page-title">
        <view  class="page-title-line"></view>
        <view  class="page-title-text">{{title}}</view>
    </view>
</view>