<view  class="container">
    <view  class="page-body">
        <onekit_button  bindtap="showNavigationBarLoading" type="primary" hover-stop-propagation="true">显示加载动画</onekit_button>
        <onekit_button  bindtap="hideNavigationBarLoading" type="primary" hover-stop-propagation="true">隐藏加载动画</onekit_button>
    </view>
    <view  class="page-title">
        <view  class="page-title-line"></view>
        <view  class="page-title-text">{{title}}</view>
    </view>
</view>