<view  class="container">
    <view  class="page-body">
        <onekit_button  bindtap="navigateTo" type="primary" hover-stop-propagation="true">跳转新页面</onekit_button>
        <onekit_button  bindtap="navigateBack" type="primary" hover-stop-propagation="true">返回上一页</onekit_button>
        <onekit_button  bindtap="redirectTo" type="primary" hover-stop-propagation="true">在当前页面打开</onekit_button>
    </view>
    <view  class="page-title">
        <view  class="page-title-line"></view>
        <view  class="page-title-text">{{title}}</view>
    </view>
</view>