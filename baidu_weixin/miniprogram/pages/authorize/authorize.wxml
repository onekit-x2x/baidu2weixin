<view  class="container">
    <view  class="page-body">
        <onekit_button  wx:for-item="item" wx:key="item" wx:for="{{items}}" id="{{item.id}}" bindtap="btnClick" type="primary" hover-stop-propagation="true">{{item.name}}</onekit_button>
    </view>
    <view  class="page-title">
        <view  class="page-title-line"></view>
        <view  class="page-title-text">{{title}}</view>
    </view>
</view>