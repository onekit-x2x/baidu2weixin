<view  class="container">
    <view  class="page-body">
        <view  class="page-item">
            <view  class="info">{{originData}}</view>
            <onekit_button  id="setClipboardData" class="btn" bindtap="btnClick" type="primary" hover-stop-propagation="true">复制</onekit_button>
        </view>
        <view  class="page-item">
            <view  class="info">{{clipboardData}}</view>
            <onekit_button  id="getClipboardData" class="btn" bindtap="btnClick" type="primary" hover-stop-propagation="true">粘贴</onekit_button>
        </view>
    </view>

    <view  class="page-title">
        <view  class="page-title-line"></view>
        <view  class="page-title-text">{{title}}</view>
    </view>
</view>