<view  class="livewrap">
    <live-player  src="{{src}}" autoplay="{{autoplay}}" object-fit="{{objectFit}}" background-mute="{{backgroundMute}}" muted="{{muted}}" min-cache="{{minCache}}" max-cache="{{maxCache}}" bindstatechange="statechange" bindnetstatus="netstatus" bindfullscreenchange="fullscreenchange" id="myLive"></live-player>
</view>
<view  class="wrap">
    <view  class="sub-item" bindtap="livePlay">开始播放 play</view>
    <view  class="sub-item" bindtap="liveStop">停止播放 stop</view>
    <view  class="sub-item" bindtap="liveMute">静音</view>
    <view  class="sub-item" bindtap="changeSrc">更换src</view>
    <view  class="sub-item" bindtap="backgroundMute">后台静音</view>
    <view  class="sub-item" bindtap="objectFit">object-fit改变</view>
    <view  class="sub-item" bindtap="changeFull">进入全屏</view>
</view>
